{
  "expo": {
    "name": "Poultry Feed Calculator",
    "slug": "chicken-feed-calculator",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourorg.chickenfeed",
      "buildNumber": "1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yourorg.chickenfeed",
      "versionCode": 1,
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.VIBRATE",
        "android.permission.WAKE_LOCK"
      ]
    },

    "plugins": [
      [
        "expo-router",
        {
          "root": "./app"
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/splash.png",
          "imageWidth": 200
        }
      ],
      [
        "expo-notifications",
        {
          "color": "#0284c7",
          "defaultChannel": "feeding-reminders"
        }
      ]
    ],
    "scheme": "chicken-feed-calculator",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "your-project-id-here"
      },
      "appEnv": "production"
    },
    "owner": "your-expo-username",
    "updates": {
      "url": "https://u.expo.dev/your-project-id-here",
      "enabled": true
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}